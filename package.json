{
  "name": "mes-r018-analysis",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:sandbox": "wrangler pages dev dist --d1=mes-r018-analysis-production --local --ip 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "wrangler pages dev",
    "deploy": "npm run build && wrangler pages deploy dist --project-name mes-r018-analysis",
    "deploy:prod": "npm run build && wrangler pages deploy dist --project-name mes-r018-analysis",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:migrate:local": "wrangler d1 migrations apply mes-r018-analysis-production --local",
    "db:migrate:prod": "wrangler d1 migrations apply mes-r018-analysis-production",
    "db:console:local": "wrangler d1 execute mes-r018-analysis-production --local",
    "db:console:prod": "wrangler d1 execute mes-r018-analysis-production",
    "git:init": "git init && git add . && git commit -m 'Initial commit'",
    "git:commit": "git add . && git commit -m",
    "git:status": "git status",
    "git:log": "git log --oneline"
  },
  "dependencies": {
    "hono": "^4.11.9"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250705.0",
    "@hono/vite-cloudflare-pages": "^0.4.2",
    "vite": "^6.3.5",
    "wrangler": "^4.4.0",
    "typescript": "^5.0.0"
  }
}